{
    "name": "norberttech/symfony-process-executor",
    "description": "Symfony Process Component on steroids, async/sync execution chain.",
    "type": "library",
    "autoload": {
        "psr-4": {
            "NorbertTech\\SymfonyProcessExecutor\\": "src/NorbertTech/SymfonyProcessExecutor"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "": "tests"
        }
    },
    "require": {
        "php": ">=7.4.2",
        "symfony/process": "^3.4|^4.4|^5.0",
        "aeon-php/process": "^1.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1",
        "phpstan/phpdoc-parser": "^0.4.4",
        "vimeo/psalm": "^3.11",
        "phpstan/phpstan": "^0.12.19",
        "friendsofphp/php-cs-fixer": "2.16.3"
    },
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts" : {
        "cs:php:fix": "php-cs-fixer fix",
        "build": [
            "@tests",
            "@static:analyze"
        ],
        "tests" : [
            "phpunit -c phpunit.xml"
        ],
        "static:analyze": [
            "php-cs-fixer fix --dry-run",
            "phpstan analyse",
            "psalm"
        ]
    }
}
